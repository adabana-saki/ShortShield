{
  "platform": "tiktok",
  "version": "1.0.0",
  "lastUpdated": "2025-01-01",
  "selectors": [
    {
      "selector": "[data-e2e=\"recommend-list-item-container\"]",
      "description": "For You page video item",
      "enabled": true,
      "priority": 10
    },
    {
      "selector": "[class*=\"DivItemContainer\"]",
      "description": "Video feed item container",
      "enabled": true,
      "priority": 9
